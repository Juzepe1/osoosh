<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_event_certificate" model="ir.sequence">
            <field name="name">Event Certificate</field>
            <field name="code">event.certificate</field>
            <field name="prefix">2017</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_event_accreditation" model="ir.sequence">
            <field name="name">Event Accreditation</field>
            <field name="code">event.accreditation</field>
            <field name="prefix">A</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
